<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sistemadelicencias.controller.PopUpController">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="50.0" layoutY="50.0" prefHeight="300.0" prefWidth="500.0" spacing="20">
            <Text fx:id="mensaje" wrappingWidth="500.0" textAlignment="CENTER">
                <font>
                    <Font name="System" size="15.0" />
                </font>
            </Text>
            <Region prefHeight="100.0" />
            <Button  fx:id="botonAceptar" onAction="#botonAceptar" mnemonicParsing="false" prefHeight="63.0" prefWidth="180.0" text="ACEPTAR">
                <font>
                    <Font name="System Bold" size="15.0" />
                </font>
            </Button>
        </VBox>
    </children>
</AnchorPane>